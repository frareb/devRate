Package: devRate
Type: Package
Title: Quantify the Relationship Between Development Rate and Temperature in
    Ectotherms
Version: 0.1.7
Authors@R: c(
    person("Francois", "Rebaudo", role = c("aut", "cre"), comment = "2016-2018",  
      email = "francois.rebaudo@ird.fr"),
